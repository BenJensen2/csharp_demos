@model Guide;

<!DOCTYPE html>
<html lang="en">
<head>
  <meta charset="UTF-8">
  <meta name="viewport" content="width=device-width, initial-scale=1.0">
  <title>Document</title>
</head>
<body>
  <a href="/">Home</a>
  <h2>Welcome @Model.Traveler.FullName()</h2>

  <ul>
    @foreach (Destination destination in Model.TravelDestinations)
    {
      <li>
        <a href="/travel/@destination.Name">@destination.Name</a>
      </li>
    }
  </ul>
</body>
</html>